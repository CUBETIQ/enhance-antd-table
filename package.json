{
  "name": "enhance-antd-table",
  "version": "1.0.8",
  "description": "enhance-antd-table antd-table @antd",
  "author": "LyhourChhen",
  "license": "MIT",
  "repository": "LyhourChhen/enhance-antd-table",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "yarn gen && microbundle-crl --no-compress --format modern,cjs",
    "start": "yarn gen && microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "yarn gen && run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "install-pk": "yarn install",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "docs-server": "styleguidist server",
    "docs-build": "styleguidist build",
    "gen": "pdfmakefg assets/ node_modules/pdfmake/build/vfs_fonts.js"
  },
  "peerDependencies": {
    "antd": "3.26.18",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/pdfmake": "^0.1.14",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "pdfmake-font-generator": "^1.0.1",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-styleguidist": "^11.0.8",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@types/styled-components": "^5.1.1",
    "@types/uuid": "^8.0.0",
    "antd": "^4.4.3",
    "moment": "^2.27.0",
    "pdfmake": "^0.1.67",
    "react-to-print": "^2.9.0",
    "styled-components": "^5.1.1",
    "uuid": "^8.2.0"
  }
}
